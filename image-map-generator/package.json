{
  "name": "imagemaps",
  "version": "1.0.0",
  "description": "jQuery plugin which can graphically build area maps on an image",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "prepublishOnly": "npm run docs",
    "eslint": "eslint --report-unused-disable-directives .",
    "rollup": "rollup -c",
    "open-docs": "opn http://localhost:8041/docs/jsdoc/",
    "open": "opn http://localhost:8041/demo/",
    "start": "static -p 8041",
    "docs": "rm -rf docs/jsdoc/*;jsdoc --pedantic -c docs/jsdoc-config.js .",
    "test": "npm run eslint && npm run rollup && testcafe chrome test/**/*.js"
  },
  "repository": {
